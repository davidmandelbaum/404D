extends layout

block content
  div#links
 
  script(type='text/javascript').
      var list = !{JSON.stringify(list)}
      var i = 0;
 
      for (i = 0; i < list.length; i++){
          var html = "<a href='/trial/" + list[i]._id + "'>" + list[i].username + ": " + list[i].datetime + "</a><br />";
          $("#links").append(html);
      }


